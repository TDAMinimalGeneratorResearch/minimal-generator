@article{erdos-renyi-RP2,
author = {Costa, Armindo and Farber, Michael and Horak, Danijela},
title = {Fundamental groups of clique complexes of random graphs},
journal = {Transactions of the London Mathematical Society},
volume = {2},
number = {1},
pages = {1-32},
doi = {https://doi.org/10.1112/tlms/tlv001},
url = {https://londmathsoc.onlinelibrary.wiley.com/doi/abs/10.1112/tlms/tlv001},
eprint = {https://londmathsoc.onlinelibrary.wiley.com/doi/pdf/10.1112/tlms/tlv001},
abstract = {We study fundamental groups of clique complexes associated to random Erdős–Rényi graphs Γ. We establish thresholds for a number of properties of fundamental groups of these complexes XΓ. In particular, if p=nα, then we show that gdim(π1(XΓ))=cd(π1(XΓ))=1ifα<−12,gdim(π1(XΓ))=cd(π1(XΓ))=2if−12<α<−1130,gdim(π1(XΓ))=cd(π1(XΓ))=∞if−1130<α<−13, asymptotically almost surely (a.a.s.), where gdim and cd denote the geometric dimension and cohomological dimension correspondingly. It is known that the fundamental group π1(XΓ) is trivial for α>−13. We prove that for −1130<α<−13 the fundamental group π1(XΓ) has 2-torsion but has no m-torsion for any given prime m⩾3. We also prove that aspherical subcomplexes of the random clique complex XΓ satisfy the Whitehead conjecture, that is, all their subcomplexes are also aspherical, a.a.s.},
year = {2015}
}
